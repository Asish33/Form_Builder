generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int    @id @default(autoincrement())
  password  String
  firstName String
  lastName  String
  email     String @unique
}

model Form {
  id       Int  @id @default(autoincrement())
  content  Json
}

